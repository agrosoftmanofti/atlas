@inherits ModulesViewBase

@if (!_loaded)
{
    <Loading Title="Modules" Message="Loading..." />
}
else
{
    <GenericTableView T="Module" DisplayHeader="@PageArgs?.DisplayName" 
                Source="@_modules" Fields="@_fields" IdentifierField="@_identifierField" 
                RoutingPage="@PageArgs?.RoutingPage" RoutingComponentCode="@PageArgs?.RoutingComponentCode" />
}