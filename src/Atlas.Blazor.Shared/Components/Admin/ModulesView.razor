@inherits ModulesViewBase

@if (!_loaded)
{
    <Loading Title="Modules" Message="Loading..." />
}
else
{
    <GenericTableView T="Module" DisplayHeader="@ComponentArgs?.DisplayName" 
                Source="@_modules" Fields="@_fields" IdentifierField="@_identifierField" 
                RoutingPage="@ComponentArgs?.RoutingPage" RoutingComponentCode="@ComponentArgs?.RoutingComponentCode" />
}