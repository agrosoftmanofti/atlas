@typeparam T where T : class, new()
@inherits ModelPropertyRolloutBase<T>

@if(_modelPropertyRenders != null)
{
    <MudStack Spacing="4">

        @foreach (ModelPropertyRender<T> modelPropertyRender in _modelPropertyRenders)
        {
            @RenderView(modelPropertyRender)
        }

    </MudStack>
}